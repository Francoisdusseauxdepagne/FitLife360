{% extends 'base.html.twig' %}

{% block title %}Créer un plan alimentation{% endblock %}

{% block body %}
    <h1>Créer un plan alimentation</h1>

    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.dateDebut) }}
        {{ form_row(form.dateFin) }}
        {{ form_row(form.idProfile) }}
        <button type="submit" class="btn btn-primary">Créer le plan alimentaire</button>
    {{ form_end(form) }}
{% endblock %}